<div class="dashboard-wrapper mt-5">
            <div class="container-fluid  dashboard-content">
                <!-- ============================================================== -->
                <!-- pageheader -->
                <!-- ============================================================== -->
                <div class="row">
                    <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                        <div class="page-header">
                            <h2 class="pageheader-title">Rooms</h2>
                            <p class="pageheader-text"></p>
                            <div class="page-breadcrumb">
                                <nav aria-label="breadcrumb">
                                    <ol class="breadcrumb">
                                        <li class="breadcrumb-item"><a href="#" class="breadcrumb-link">Dashboard</a></li>
                                        
                                    </ol>
                                </nav>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- ============================================================== -->
                <!-- end pageheader -->
                <!-- ============================================================== -->
                <div class="row">
                    <!-- ============================================================== -->
                    <!-- basic table  -->
                    <!-- ============================================================== -->
                    <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                        <div class="card">
                            
                            <div class="card-body">
                                <div class="table-responsive">
                                    <table class="table table-striped table-bordered first" id="data_table">
                                        <thead>
                                            <tr>
                                                <th>Name</th>
                                                <th>Hotel</th>
                                                <th>Price</th>
                                                <th>City</th>
                                                <th>Category</th>
                                                <th>Contact</th>
                                               
                                                
                                            </tr>
                                        </thead>
                                        <tbody>
                                              {{#each rooms}}
                                            <tr>
                                              
                                                <td>{{this.roomname}}</td>
                                                <td>{{this.hotelname}}</td>
                                                <td>{{this.roomprice}}</td>
                                                <td>{{this.city}}</td>
                                                <td>{{this.category}}</td>
                                                <td>{{this.mobile}}</td>
                                                
                                            </tr>
                                            {{/each}}
                                           
                                        </tbody>
                                        
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- ============================================================== -->
                    <!-- end basic table  -->
                    <!-- ============================================================== -->
                </div>
               
                
                
                
            </div>
            <!-- ============================================================== -->
            <!-- footer -->
            <!-- ============================================================== -->
            <div class="footer">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-xl-6 col-lg-6 col-md-12 col-sm-12 col-12">
