<div class="dashboard-wrapper mt-5">
            <div class="container-fluid  dashboard-content">
                <!-- ============================================================== -->
                <!-- pageheader -->
                <!-- ============================================================== -->
                <div class="row">
                    <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                        <div class="page-header">
                            <h2 class="pageheader-title">Blocked Users</h2>
                            <p class="pageheader-text"></p>
                            <div class="page-breadcrumb">
                                <nav aria-label="breadcrumb">
                                    <ol class="breadcrumb">
                                        <li class="breadcrumb-item"><a href="#" class="breadcrumb-link">Dashboard</a></li>
                                        
                                    </ol>
                                </nav>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- ============================================================== -->
                <!-- end pageheader -->
                <!-- ============================================================== -->
                <div class="row">
                    <!-- ============================================================== -->
                    <!-- basic table  -->
                    <!-- ============================================================== -->
                    <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                        <div class="card">
                            
                            <div class="card-body">
                                <div class="table-responsive">
                                    <table class="table table-striped table-bordered first" id="data_table">
                                        <thead>
                                            <tr>
                                                <th>Name</th>
                                                <th>E-mail</th>
                                                
                                                <th>Contact</th>
                                                <th class="text-center">Actions</th>
                                                
                                            </tr>
                                        </thead>
                                        <tbody>
                                              {{#each users}}
                                            <tr>
                                              
                                                <td>{{this.name}}</td>
                                                <td>{{this.username}}</td>
                                                <td>{{this.mobile}}</td>
                                                
                                                <td class=" text-center">
                                                    <a href="/admin/unblock-user/{{this._id}}" class="btn btn-warning btn-sm text-white" >Unblock</a>
                                                    <a href="/admin/delete-user/{{this._id}}" class="btn btn-danger btn-sm text-white">Delete</a>
                                                </td>
                                                
                                            </tr>
                                            {{/each}}
                                           
                                        </tbody>
                                        
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- ============================================================== -->
                    <!-- end basic table  -->
                    <!-- ============================================================== -->
                </div>
               
                
                
                
            </div>
